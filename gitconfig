[alias]
    unstage = reset HEAD
    s = status
    c = commit -s
    co = checkout
    br = branch
    changelog = log --pretty='format:* %s'
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    authors = log --all --format='%aN <%aE>'
